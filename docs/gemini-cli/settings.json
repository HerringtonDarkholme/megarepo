{
  "theme": "GitHub",
  "autoAccept": false,
  "vimMode": false,
  "sandbox": false,
  "hideTips": false,
  "hideBanner": false,
  "usageStatisticsEnabled": true,
  "showLineNumbers": true,
  "maxSessionTurns": -1,
  "fileFiltering": {
    "respectGitIgnore": true,
    "enableRecursiveFileSearch": true
  },
  "telemetry": {
    "enabled": false,
    "target": "local",
    "otlpEndpoint": "http://localhost:4317",
    "logPrompts": true
  },
  "checkpointing": {
    "enabled": false
  },
  "preferredEditor": "vscode"
}