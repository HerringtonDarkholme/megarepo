# Gemini CLI Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Your Gemini API key
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Specify the default Gemini model
GEMINI_MODEL=gemini-2.0-flash-exp

# Optional: Google Cloud configuration for Vertex AI
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/credentials.json

# Optional: Gemini CLI behavior settings
GEMINI_SANDBOX=false
NO_COLOR=
CLI_TITLE=Megarepo AI Development

# Optional: Telemetry configuration
OTLP_GOOGLE_CLOUD_PROJECT=your_telemetry_project_id

# Optional: Debug mode (excluded from project .env by default)
# DEBUG=true
# DEBUG_MODE=1